<?xml version='1.0' encoding='utf-8'?>
<plugin id="cordova-plugin-printer-sdk" version="0.0.2" xmlns="http://apache.org/cordova/ns/plugins/1.0" xmlns:android="http://schemas.android.com/apk/res/android">
	<name>PrinterSDK</name>
	<DESCRIPCION>Printer sdk mini thermal printer</DESCRIPCION>
	<js-module name="PrinterSDK" src="www/PrinterSDK.js">
		<clobbers target="cordova.plugins.PrinterSDK" />
	</js-module>
	<platform name="android">
		<config-file parent="/*" target="res/xml/config.xml">
			<feature name="PrinterSDK"><param name="android-package" value="cordova-plugin-Printer-SDK.PrinterSDK" /></feature>
		</config-file>
		<config-file parent="/*" target="AndroidManifest.xml">
			<uses-permission android:name="android.permission.READ_CALENDAR" />
		</config-file>
		<source-file src="src/android/PrinterSDK.java" target-dir="src/cordova-plugin-Printer-SDK/PrinterSDK" />
	</platform>
	<platform name="ios">
		<config-file parent="/*" target="config.xml">
			<feature name="PrinterSDK">
				<param name="ios-package" value="PrinterSDK" />
				<param name="onload" value="true" />
			</feature>
		</config-file>
		<header-file src="src/ios/PrinterSDK.h" />
		<source-file src="src/ios/PrinterSDK.m" />
		<source-file src="src/ios/libPrinterSDK.a" />	
	</platform>
</plugin>